machine Simple1 {
  var a;
  init state Start {
    entry (a) {
      this.a = a;
    }
  }
}
machine Simple2 {
  vars a, b;
  init state Start {
    entry (a) {
      this.b = new Simple1(a);
    }
  }
}
init machine ExternTest {
  init state Start {
    entry {
      var retval = extern echo(new Simple2(1)); // echo an object
      print(retval.b.a); // 1
    }
  }
}
